#include <iostream>
using namespace std;
class Box{
    friend void printlength(Box);
    private:
    int length=0;
    public:
    Box(){
        length=10;
    }
    
};
    void printlength(Box b){
        cout<<"length= "<<b.length<<endl;
    };


int main() {
    Box b;
    printlength(b);

    
 
    return 0;
}
